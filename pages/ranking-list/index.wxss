.pages-rank-list-index{
  width: 100%;
  height: 100vh;
  background-image: url("https://march.yuanian.com/static/assets/img/ranking-bg(1).jpg");
  background-size: 100% 100vh;
  background-repeat: no-repeat;
}
.rule-btn{
  width: 120rpx;
  height: 60rpx;
  float: right;
  margin-right: 35rpx;
  padding-top: 30rpx;
}
.title{
  width: 390rpx;
	height: 94rpx;
  margin: 0 auto;
  /* clear:both; */
  padding-top: 65rpx;
}
.sub-title{
  font-size: 26rpx;
  color: #4e4e4e;
  text-align: center;
  margin: 10rpx 0;
}
.content-box{
  width: 680rpx;
	height: 890rpx;
  background-image: url("https://march.yuanian.com/static/assets/img/ranking-floor.png");
  background-size: 680rpx 900rpx;
  background-repeat: no-repeat;
  margin: 0 auto;
}
.headline{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 70rpx 80rpx 20rpx;
}
.line{
  height: 20rpx;
  width: 90rpx;
	background-color: #CFF1B9;
  font-size: 38rpx;
  font-weight: bold;
  color: #4e4e4e;
  line-height: 4rpx;
  text-align: center;
}
.list-box{
  width: 520rpx;
  margin: 0 auto;
}
.each-list{
  height: 60rpx;
  border-bottom: solid 2rpx #b2b2b2;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.value{
  width: 90rpx;
  text-align: center;
  font-size: 26rpx;
	color: #4e4e4e
}
.oneself{
  width: 522rpx;
	height: 74rpx;
  background-image: url("https://march.yuanian.com/static/assets/img/icon/ranking-base.png");
  background-size:  522rpx 74rpx;
  background-repeat: no-repeat;
  margin-top: 35rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.img-icon{
  width: 46rpx;
	height: 45rpx;
  padding-top: 15rpx;
}